{
  "name": "@mikecousins/react-pdf",
  "version": "5.3.1",
  "description": "Simple React component to wrap up PDF.js. The easiest way to render PDFs in your React app.",
  "author": "mikecousins",
  "license": "MIT",
  "repository": "mikecousins/react-pdf-js",
  "main": "dist/index.js",
  "module": "dist/react-pdf.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint src",
    "netlify": "yarn && yarn build && cd example && yarn && yarn build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@testing-library/react": "9.2.0",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "husky": "3.0.5",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "tsdx": "0.9.2",
    "tslib": "1.10.0",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@bundled-es-modules/pdfjs-dist": "2.2.228-alpha1"
  }
}
